{
    "type": "package",
    "name": "ggb/numeral-elm",
    "version": "1.4.1",
    "summary": "Elm module for (advanced) number formatting. Numeral.js port to Elm",
    "repository": "https://github.com/ggb/numeral-elm.git",
    "license": "MIT",
    "source-directories": [
        "src/"
    ],
    "exposed-modules": [
        "Numeral",
        "Language",
        "Languages.English",
        "Languages.BritishEnglish",
        "Languages.German",
        "Languages.French",
        "Languages.Japanese",
        "Languages.Italian",
        "Languages.Russian",
        "Languages.Ukrainian"
    ],
    "dependencies": {
        "elm/core": "1.0.0 <= v < 2.0.0",
	"elm/regex": "1.0.0 <= v < 2.0.0"
    },
    "test-dependencies": {},
    "elm-version": "0.19.0 <= v < 0.20.0"
}
